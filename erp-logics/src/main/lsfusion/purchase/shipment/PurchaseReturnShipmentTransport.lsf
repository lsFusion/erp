MODULE PurchaseReturnShipmentTransport;

REQUIRE PurchaseReturnShipment, Transport,
        PurchaseReturnInvoiceTransport;

NAMESPACE PurchaseReturn;

@defineDocumentTransport(userShipment, s, headerTabbedPane);

truck = ABSTRACT VALUE Truck (Shipment);
truck (ShipmentDetail d) = truck(shipment(d));

truck (UserShipment s) += truck(s);
truck (InvoiceShipment s) += truck(invoice(s));

