MODULE StatusUPD;

REQUIRE System;

NAMESPACE StatusUPD;

CLASS StatusUPD 'Статус'{
    one '1',
    two '2'       
}

name 'Наименование' (StatusUPD t) = staticCaption(t) IF t IS StatusUPD CHARWIDTH 15;  

FORM StatusUPD 'Статус УПД'
    OBJECTS c = StatusUPD
    PROPERTIES(c) READONLY name
    ORDERS name(c)
    LIST StatusUPD OBJECT c
;
DESIGN StatusUPD{
    BOX {
        size = (400, 400);
    }
}
